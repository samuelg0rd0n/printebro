	<meta name="keywords" content="tisk, ofsetový, digitální, knihařské zpracování, Bzenec, dokončovací zpracování, billboardy, letáky, katalogy, brožury, samolepky" />

	<meta property="og:url" content="http://www.printebro.cz/">
	<meta property="og:title" content="Printebro - Jsme tu pro vás a vaše tiskoviny">
	<meta property="og:description" content="Nabízíme plnobarevný tisk (ofsetový i digitální), dokončovací zpracování, knihařské zpracování, billboardy, polepy a spoustu dalšího (od 1 ks po tisíce).">
	<meta property="og:image" content="http://www.printebro.cz/android-chrome-384x384.png">
	<meta property="og:image:width" content="384">
	<meta property="og:image:height" content="384">
	<meta property="og:type" content="website">

