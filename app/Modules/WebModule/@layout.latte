</head>

<body>
	{snippet flashes}
		<div n:foreach="$flashes as $flash" n:class="alert, 'alert-' . $flash->type">{$flash->message}</div>
	{/snippet}

	{include content}

	{block scripts}

		{* JS COMPONENTS *}
		<script src="{$basePath}/dist/js/app.js{v}"></script>

		{ifset componentJs}{include componentJs}{/ifset}
	{/block}
</body>
</html>
